##### file_contexts
/dev/video0			u:object_r:camera_device:s0
/dev/dmmu			u:object_r:gpu_device:s0
/dev/ipu0			u:object_r:gpu_device:s0
/dev/dri/.+			u:object_r:gpu_device:s0
/dev/sw_sync			u:object_r:gpu_device:s0
/dev/jz-vpu			u:object_r:video_device:s0
/dev/pvr_sync			u:object_r:gpu_device:s0
/dev/ttyS2			u:object_r:serial_device:s0

/system/vendor/bin/pvrsrvctl	u:object_r:pvrsrvctl_exec:s0
/system/bin/setup_fs		u:object_r:setup_fs_exec:s0

/dev/ubi.*			u:object_r:mtd_device:s0

/dev/block/mmcblk0p2		u:object_r:system_block_device:s0
/dev/block/mmcblk0p3		u:object_r:cache_block_device:s0
/dev/block/mmcblk0p4		u:object_r:userdata_block_device:s0

/dev/block/zram0		u:object_r:swap_block_device:s0
